{
  "name": "photoleuchten",
  "version": "2.4.2",
  "description": "generator for photoleuchten.com",
  "author": "Steffen Baer <stbaer.mail@gmail.com>",
  "private": true,
  "license": "UNLICENCE",
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "prelint": "echo 'Running eslint...' && exit 0",
    "lint": "eslint -c ./.eslintrc.js --ext .js,.vue ./",
    "deploy": "gf-release"
  },
  "dependencies": {
    "nuxt": "1.0.0-rc11"
  },
  "devDependencies": {
    "@nextindex/next-scss": "1.2.0",
    "accept-language-parser": "1.4.1",
    "axios": "0.16.2",
    "babel-core": "6.26.0",
    "babel-eslint": "8.0.1",
    "babel-plugin-component": "0.10.1",
    "eslint": "4.8.0",
    "eslint-config-standard": "10.2.1",
    "eslint-loader": "1.9.0",
    "eslint-plugin-html": "3.2.2",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-node": "5.2.0",
    "eslint-plugin-promise": "^3.4.1",
    "eslint-plugin-standard": "3.0.1",
    "flexboxgrid": "^6.3.1",
    "gf-release": "1.4.5",
    "less": "^2.7.2",
    "less-loader": "4.0.5",
    "loader-utils": "1.1.0",
    "material-components-web": "0.22.0",
    "node-sass": "4.5.3",
    "normalize.css": "7.0.0",
    "shelljs": "latest",
    "vue-i18n": "7.3.1"
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 2%",
    "last 2 versions",
    "not ie <= 9"
  ],
  "releaseConfig": {
    "buildCommand": "node scripts/build.js && git add -A && git commit -am \"updated docs folder\"",
    "versionFiles": [
      "package.json"
    ],
    "commitMessagesExclude": [
      "Merge tag",
      "Merge branch 'release",
      "Merge branch 'develop' into",
      "bumped",
      "(--)"
    ],
    "commitBaseUrl": "https://github.com/bear2s/photoleuchten/commits",
    "historyFile": "History.md"
  }
}