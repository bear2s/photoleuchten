{
  "name": "photoleuchten",
  "version": "2.0.12",
  "description": "generator for photoleuchten.com",
  "author": "Steffen Baer <stbaer.mail@gmail.com>",
  "private": true,
  "license": "UNLICENCE",
  "scripts": {
    "cp:pl-imgs": "node scripts/mvImages",
    "predev": "npm run cp:pl-imgs",
    "prebuild": "npm run cp:pl-imgs",
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint  --ext .js,.vue pages layout scripts components --ignore-path .gitignore ",
    "precommit": "npm run lint"
  },
  "dependencies": {
    "nuxt": "latest"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "babel-plugin-component": "0.9.1",
    "eslint": "3.19.0",
    "eslint-config-standard": "10.2.0",
    "eslint-loader": "1.7.1",
    "eslint-plugin-html": "^2.0.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-node": "4.2.2",
    "eslint-plugin-promise": "^3.4.1",
    "eslint-plugin-standard": "3.0.1",
    "flexboxgrid": "^6.3.1",
    "less": "^2.7.2",
    "less-loader": "4.0.3",
    "normalize.css": "^6.0.0",
    "pl-images": "file:../pl-images#1.1.6",
    "shelljs": "latest"
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 2%",
    "last 2 versions",
    "not ie <= 9"
  ],
  "releaseConfig": {
    "buildCommand": "npm run generate && rm -rf docs && cp -R dist docs && echo 'photoleuchten.com' > docs/CNAME && git add -A && git commit -am 'updated docs folder'",
    "versionFiles": [
      "package.json"
    ],
    "commitMessagesExclude": [
      "Merge tag",
      "Merge branch 'release",
      "Merge branch 'develop' into",
      "bumped",
      "(--)"
    ],
    "commitBaseUrl": "https://github.com/bear2s/photoleuchten/commits",
    "historyFile": "History.md"
  }
}