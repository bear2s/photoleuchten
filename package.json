{
  "name": "photoleuchten",
  "version": "2.7.7",
  "description": "generator for photoleuchten.com",
  "author": "Steffen Baer <stbaer.mail@gmail.com>",
  "private": true,
  "license": "UNLICENCE",
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate && echo \"photoleuchten.com\" > docs/CNAME",
    "prelint": "echo 'Running eslint...' && exit 0",
    "lint": "eslint -c ./.eslintrc.js --ext .js,.vue ./",
    "deploy": "gf-release"
  },
  "dependencies": {
    "@nuxtjs/sitemap": "0.0.3",
    "accept-language-parser": "1.5.0",
    "webfontloader": "1.6.28"
  },
  "devDependencies": {
    "babel-eslint": "8.2.2",
    "babel-plugin-transform-imports": "1.5.0",
    "eslint": "4.19.1",
    "eslint-config-standard": "11.0.0",
    "eslint-loader": "2.0.0",
    "eslint-plugin-html": "4.0.2",
    "eslint-plugin-import": "2.10.0",
    "eslint-plugin-node": "6.0.1",
    "eslint-plugin-promise": "3.7.0",
    "eslint-plugin-standard": "3.0.1",
    "eslint-plugin-vue": "4.4.0",
    "gf-release": "1.4.5",
    "loader-utils": "1.1.0",
    "nuxt": "1.4.0",
    "shelljs": "0.8.1",
    "stylus": "0.54.5",
    "stylus-loader": "3.0.2",
    "vue-i18n": "7.6.0",
    "vuetify": "1.0.13",
    "vuetify-loader": "0.1.1"
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 3 versions",
    "not ie <= 10"
  ],
  "releaseConfig": {
    "buildCommand": "npm run generate && git add -A && git commit -am \"updated docs folder\"",
    "versionFiles": [
      "package.json"
    ],
    "commitMessagesExclude": [
      "Merge tag",
      "Merge branch 'release",
      "Merge branch 'develop' into",
      "bumped",
      "(--)"
    ],
    "commitBaseUrl": "https://github.com/bear2s/photoleuchten/commits"
  }
}
