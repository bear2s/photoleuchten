{
  "name": "photoleuchten",
  "version": "2.0.0",
  "description": "generator for photoleuchten.com",
  "author": "Steffen Baer <stbaer.mail@gmail.com>",
  "private": true,
  "license": "UNLICENCE",
  "scripts": {
    "clean:static": "rm -rf static/img",
    "cp:pl-imgs": "npm run clean:static && cp -R node_modules/pl-images/dist static/img",
    "predev": "npm run cp:pl-imgs",
    "prebuild": "npm run cp:pl-imgs",
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint"
  },
  "dependencies": {
    "nuxt": "latest"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "eslint": "^3.15.0",
    "eslint-config-standard": "^7.1.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-html": "^2.0.0",
    "eslint-plugin-promise": "^3.4.1",
    "eslint-plugin-standard": "^2.0.1",
    "flexboxgrid": "^6.3.1",
    "less": "^2.7.2",
    "less-loader": "^4.0.2",
    "normalize.css": "^6.0.0",
    "pl-images": "file:../pl-images#1.0.3",
    "vue-lazyload": "^1.0.1"
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 2%",
    "last 2 versions",
    "not ie <= 9"
  ],
  "releaseConfig": {
    "buildCommand": "npm run generate && rm -rf docs && cp -R dist docs && echo 'photoleuchten.com' > docs/CNAME && git add -A && git commit -am 'updated docs folder'",
    "versionFiles": [
      "package.json"
    ],
    "commitMessagesExclude": [
      "Merge tag",
      "Merge branch 'release",
      "Merge branch 'develop' into",
      "bumped",
      "(--)"
    ],
    "commitBaseUrl": "https://github.com/bear2s/photoleuchten/commits",
    "historyFile": "History.md"
  }
}