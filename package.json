{
  "name": "photoleuchten",
  "version": "2.9.0",
  "description": "generator for photoleuchten.com",
  "author": "Steffen Baer <stbaer.mail@gmail.com>",
  "private": true,
  "license": "UNLICENCE",
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate && echo \"photoleuchten.com\" > docs/CNAME",
    "prelint": "echo 'Running eslint...' && exit 0",
    "lint": "eslint -c ./.eslintrc.js --ext .js,.vue ./",
    "deploy": "gf-release"
  },
  "dependencies": {
    "@nuxtjs/google-gtag": "1.0.4",
    "@nuxtjs/sitemap": "0.1.1",
    "accept-language-parser": "1.5.0",
    "webfontloader": "1.6.28"
  },
  "devDependencies": {
    "@vue/eslint-config-standard": "4.0.0",
    "babel-eslint": "10.0.1",
    "babel-plugin-transform-imports": "1.5.1",
    "eslint": "5.9.0",
    "eslint-loader": "2.1.1",
    "eslint-plugin-vue": "4.7.1",
    "gf-release": "1.4.5",
    "loader-utils": "1.1.0",
    "nuxt": "2.3.2",
    "nuxt-trailingslash-module": "1.1.0",
    "shelljs": "0.8.3",
    "stylus": "0.54.5",
    "stylus-loader": "3.0.2",
    "vue-i18n": "8.3.2",
    "vuetify": "1.3.9",
    "vuetify-loader": "1.0.6"
  },
  "engines": {
    "node": ">= 10.0.0",
    "npm": ">= 5.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 3 versions",
    "not ie < 11"
  ],
  "releaseConfig": {
    "buildCommand": "npm run generate && git add -A && git commit -am \"updated docs folder\"",
    "versionFiles": [
      "package.json"
    ],
    "commitMessagesExclude": [
      "Merge tag",
      "Merge branch 'release",
      "Merge branch 'develop' into",
      "bumped",
      "(--)"
    ],
    "commitBaseUrl": "https://github.com/bear2s/photoleuchten/commits"
  }
}
