{
  "name": "photoleuchten",
  "version": "1.1.1",
  "description": ".",
  "author": "Steffen Baer <stbaer.mail@gmail.com>",
  "private": true,
  "scripts": {
    "clean:static": "rm -rf static/img",
    "cp:pl-imgs": "npm run clean:static && cp -R node_modules/pl-images/dist static/img",
    "predev": "npm run cp:pl-imgs",
    "prebuild": "npm run cp:pl-imgs",
    "dev": "node build/dev-server.js",
    "build": "node build/build.js",
    "lint": "eslint --ext .js,.vue src build"
  },
  "dependencies": {
    "vue": "^2.2.5"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.2.1",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.2.2",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.24.0",
    "chalk": "^1.1.3",
    "connect-history-api-fallback": "^1.3.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.27.3",
    "eslint": "^3.18.0",
    "eslint-config-standard": "7.1.0",
    "eslint-friendly-formatter": "^2.0.7",
    "eslint-loader": "^1.7.0",
    "eslint-plugin-html": "^2.0.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.14.1",
    "extract-text-webpack-plugin": "^2.0.0",
    "file-loader": "^0.10.0",
    "flexboxgrid": "^6.3.1",
    "friendly-errors-webpack-plugin": "^1.1.3",
    "function-bind": "^1.1.0",
    "gf-release": "^1.4.5",
    "html-webpack-plugin": "^2.28.0",
    "http-proxy-middleware": "^0.17.3",
    "less": "^2.7.2",
    "less-loader": "4.0.2",
    "opn": "^4.0.2",
    "optimize-css-assets-webpack-plugin": "^1.3.0",
    "ora": "^1.1.0",
    "pl-images": "file:../pl-images#1.0.2",
    "rimraf": "^2.6.0",
    "semver": "^5.3.0",
    "url-loader": "^0.5.7",
    "vue-lazyload": "^1.0.1",
    "vue-loader": "^11.3.3",
    "vue-masonry": "^0.9.4",
    "vue-parallaxy": "^1.0.1",
    "vue-style-loader": "^2.0.4",
    "vue-template-compiler": "^2.2.5",
    "webpack": "^2.3.2",
    "webpack-bundle-analyzer": "^2.2.1",
    "webpack-dev-middleware": "^1.10.0",
    "webpack-hot-middleware": "^2.16.1",
    "webpack-merge": "^4.1.0"
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 2%",
    "last 2 versions",
    "not ie <= 9"
  ],
  "releaseConfig": {
    "buildCommand": "npm run build && rm -rf docs && cp -R dist docs && echo 'photoleuchten.com' > docs/CNAME && git add -A && git commit -am 'updated docs folder'",
    "versionFiles": [
      "package.json"
    ],
    "commitMessagesExclude": [
      "Merge tag",
      "Merge branch 'release",
      "Merge branch 'develop' into",
      "bumped",
      "(--)"
    ],
    "commitBaseUrl": "https://github.com/bear2s/photoleuchten/commits",
    "historyFile": "History.md"
  }
}
